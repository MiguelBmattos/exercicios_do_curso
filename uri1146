import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int x=1;
		
		while(x!=0) {
			x = sc.nextInt();
			for(int i=1; i<=x; i++) {
				System.out.printf("%d ",i);
				if(i==x) {
					System.out.print("\n");
				}
			}
		}
		
		sc.close();
	}

}
