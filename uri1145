import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		int y = sc.nextInt();
		int qnt = 0;
		
		for(int i=1; i<=y; i++) {
			System.out.printf("%d ",i);
			qnt += 1;
			if(qnt==x) {
				System.out.print("\n");
				qnt=0;
			}
		}
		
		sc.close();
	}

}
