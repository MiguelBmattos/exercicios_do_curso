import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int a = 1;
		int b = 2;
		int c = 3;
		
		for(int i=1; i<=n; i++) {
			System.out.printf("%d ",a);
			System.out.printf("%d ",b);
			System.out.printf("%d ",c);
			System.out.println("PUM");
			a += 4;
			b += 4;
			c += 4;
		}
		
		sc.close();
	}

}
